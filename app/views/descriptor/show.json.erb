{
  "authentication": {
    "type": "jwt"
  },
  "baseUrl": "<%= base_url %>",
  "contexts": [
    "personal"
  ],
  "description": "Discover how your users perceive the support service you provide",
  "key": "<%= plugin_key %>",
  "name": "<%= plugin_name %>",
  "scopes": [
    "read",
    "write"
  ],
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "vendor": {
    "name": "Pawel Niewiadomski",
    "url": "https://pawelniewiadomski.com"
  },
  "modules": {
    "configurePage": {
      "url": "/configuration",
      "name": {
        "value": "Configuration"
      },
      "key": "config-page"
    },
    "jiraProjectAdminTabPanels": [
      {
        "key": "custom-feedback-project-config",
        "location": "projectgroup4",
        "name": {
          "value": "Customer feedback"
        },
        "conditions": [
          {
            "or": [
              {"condition": "user_is_project_admin"},
              {"condition": "user_is_admin"}
            ]
          },
          {
            "condition": "can_use_application",
            "params": {
              "applicationKey": "jira-servicedesk"
            }
          }
        ],
        "url": "/project_configurations/{project.id}/edit"
      }
    ],
    "jiraEntityProperties": [
      {
        "name": {
          "value": "Customer Feedback"
        },
        "entityType": "issue",
        "key": "customer-feedback-entity-property",
        "keyConfigurations": [
          {
            "propertyKey": "customer-feedback",
            "extractions": [
              {
                "objectName": "rating",
                "type": "number",
                "alias": "customerFeedbackRating"
              },
              {
                "objectName": "comment",
                "type": "text",
                "alias": "customerFeedbackComment"
              }
            ]
          }
        ]
      }
    ]
  }
}